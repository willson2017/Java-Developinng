package question3;

public class conditions {

	public static int checkValue(int x, int y)
	{
		if (x > y)
		{
			int t = x;
			y = x;
			x = t;
			System.out.println("x = " + x + " and y = " + y);
		}
		else
			System.out.println("Have a nice day");
	}
	public static void main(String[] args) {
		int x, y;
		

	}

}
